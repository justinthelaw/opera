{
  "name": "smarter-bullets",
  "version": "0.1.0",
  "scripts": {
    "install-all": "npx install-peerdeps --dev eslint-config-react-app && cd ./client && npm install && cd ../server && npm install",
    "client": "cd ./client && npm start",
    "server": "cd ./server && npm run start:dev",
    "database": "chmod +x ./data/database.dev.sh && ./data/database.dev.sh",
    "lint": "eslint ./ --ext '.ts,.tsx' --fix",
    "lint:security": "eslint ./ --ext '.ts,.tsx --fix'",
    "test:client": "cd ./client && npm test",
    "test:server": "cd ./server && npm test",
    "build:client": "cd ./client && npm run build",
    "build:server": "cd ./server && npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.8.8"
  }
}
