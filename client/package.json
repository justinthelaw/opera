{
    "name": "opera-client",
    "version": "1.0.0",
    "author": "justinthelaw",
    "license": "MIT",
    "main": "index.tsx",
    "homepage": "https://justinthelaw.github.io/opera/",
    "scripts": {
        "start": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
        "start:check": "NODE_ENV=development CHECK=true webpack-dev-server --config webpack.config.js",
        "build": "NODE_ENV=production webpack --config webpack.config.js",
        "lint:all": "npm run lint:general && npm run lint:security",
        "lint:general": "eslint ./ --ext '.ts,.tsx' --fix",
        "lint:security": "eslint --no-eslintrc --config security.eslintrc.yaml ./ --ext '.ts,.tsx --fix'",
        "test": "jest",
        "test:dev": "jest --runInBand --watchAll --detectOpenHandles --verbose"
    },
    "dependencies": {
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.14.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/draft-js": "^0.11.12",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.13.0",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "css-loader": "^6.7.3",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-prettier": "^5.0.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^29.4.3",
        "jest-environment-jsdom": "^29.4.3",
        "prettier": "^3.0.0",
        "prettier-plugin-organize-imports": "^3.2.2",
        "react-test-renderer": "^18.2.0",
        "style-loader": "^3.3.1",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "webpack": "^5.76.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1"
    }
}
